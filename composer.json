{
    "name": "haimdx/larasubdb",
    "description": "Laravel Integration of SUB DB API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anass Arakia",
            "email": "dryben82@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ixudra/curl": "6.*",
    },
    "autoload": {
        "psr-4": {
            "HaimDX\\LaraSubDB\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HaimDX\\LaraSubDB\\LaraSubDBServiceProvider"
            ],
            "aliases": {
                "LaraSubDB": "HaimDX\\LaraSubDB\\Facades\\LaraSubDBFacade"
            }
        }
    }
}
